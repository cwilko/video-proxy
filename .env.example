# Example environment configuration for video-proxy-python

# Server configuration
PORT=8080

# Upstream proxy configuration
UPSTREAM_PROXY_URL=http://192.168.1.79:3128
USE_UPSTREAM=true

# Logging configuration (optional)
LOG_LEVEL=INFO

# Example configurations:

# Without upstream proxy (direct internet access)
# USE_UPSTREAM=false

# With different upstream proxy
# UPSTREAM_PROXY_URL=http://proxy.example.com:8080

# Custom port
# PORT=9090

# Copy this file to .env and modify as needed:
# cp .env.example .env